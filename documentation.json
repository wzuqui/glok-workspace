            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Component, HostBinding, HostListener, OnInit } from '@angular/core';\n\n@Component({\n  selector: 'lib-glok-menu',\n  templateUrl: './glok-menu.component.html',\n  styleUrls: ['./glok-menu.component.scss']\n})\nexport class GlokMenuComponent implements OnInit {\n  @HostBinding('tabindex')\n  public tabindex = 0;\n\n  @HostBinding(\"class.aberto\")\n  public aberto = false;\n\n  constructor() { }\n\n  public ngOnInit(): void {\n  }\n\n  @HostListener(\"blur\", [\"$event\"])\n  public acaoObscurecer(evento: Event) {\n    this.aberto = false;\n  }\n\n  public acaoMenuItemClicou(evento: Event) {\n    this.aberto = false;\n    console.log(evento.currentTarget);\n  }\n}\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": ":host {\n  display: block;\n  font-family: Verdana, Geneva, Tahoma, sans-serif;\n  font-size: 14px;\n  position: relative;\n  user-select: none;\n\n  .botao {\n    background-color: purple;\n    color: #FFF;\n  }\n\n  .menu {\n    background-color: aliceblue;\n    display: none;\n    left: 0;\n    position: absolute;\n    top: 34px;\n\n    .agrupador {\n      display: flex;\n      flex-direction: column;\n\n      > div {\n        padding: 10px;\n      }\n\n      .menu-agrupador {\n        background-color: rgb(159, 198, 233);\n\n        &::after {\n          content: '▼';\n          float: right;\n        }\n      }\n\n      .menu-item {\n        &:hover {\n          cursor: pointer;\n          background-color: rgba(129, 135, 141, 0.842);\n          text-decoration: underline;\n        }\n        &:active {\n          background-color: rgba(89, 105, 121, 0.842);\n        }\n      }\n    }\n  }\n\n\n  &:focus.aberto {\n    .botao {\n      background-color: rgb(83, 0, 83);\n    }\n    .menu {\n      display: unset;\n    }\n  }\n}\n",
                    "styleUrl": "./glok-menu.component.scss"
                }
